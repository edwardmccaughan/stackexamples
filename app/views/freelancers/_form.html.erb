<%= form_for(@freelancer) do |f| %>


  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
    <div class="field">
    <%= f.label :phoneno %><br>
    <%= f.text_field :phoneno %>
  </div>
  <div class="field">
    <%= f.label :rate %><br>
    <%= f.text_field :rate %>
  </div>
  <div class="field">
    <%= f.label :joshapproved, 'Josh Approved' %><br>
    <%= f.check_box :joshapproved %>
  </div>
  <div class="field">
    <%= f.label :dateofapproval %><br>
    <%= f.text_field :dateofapproval %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
